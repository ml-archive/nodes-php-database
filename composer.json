{
    "name": "nodes/database",
    "description": "Makes it easier to work with models and repositories in Laravel",
    "keywords": [
        "nodes",
        "database",
        "laravel",
        "eloquent",
        "repository"
    ],
    "license": "MIT",
    "homepage": "http://nodesagency.com",
    "authors": [
        {
            "name": "Morten Rugaard",
            "email": "moru@nodes.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "5.2.*||5.3.*||5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||6.14.*",
        "illuminate/database": "5.2.*||5.3.*||5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||6.14.*",
        "nodes/core": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "~3.0"
    },
    "suggest": {
        "nodes/counter-cache": "Adds the concept of counter caching to your project"
    },
    "autoload": {
        "psr-4": {
            "Nodes\\Database\\": "src/"
        },
        "files": [
            "src/Support/Helpers/Query.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nodes\\Database\\Tests\\": "tests/"
        }
    }
}
